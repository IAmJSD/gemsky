<% if @embed['$type'] == 'app.bsky.embed.external' %>
    <%= render SkeetPostExternalEmbedComponent.new(
        external: @embed['external'],
        did: @did,
    ) %>
<% elsif @embed['$type'] == 'app.bsky.embed.images' %>
    <%= render SkeetPostImagesEmbedComponent.new(
        images: @embed['images'],
        did: @did,
    ) %>
<% end %>
